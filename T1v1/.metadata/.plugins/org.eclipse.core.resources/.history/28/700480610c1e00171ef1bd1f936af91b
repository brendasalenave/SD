import java.net.InetAddress;

public class Process implements Runnable{
	Pilha stack;
	int porta;
	InetAddress ip;

	
	public Process (Pilha stack, int porta, InetAddress ip){
		this.stack = stack;
		this.porta = porta;
		this.ip = ip;
	}
	
	@Override
	public void run(){
		byte[] dado; 
		
		int d = stack.pop();
		String s = String.valueOf(d);
		
		dado = s.getBytes();
		DatagramSocket ds = new DatagramSocket();
		DatagramPackage dp
	}

}

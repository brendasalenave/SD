import java.util.*;

public class Pilha {

	  private List<Integer> stack = new LinkedList<Integer>();

	  public void insere(Integer n) {
	    this.stack.add(n);
	  }

	  public Object remove() {
	    return this.objetos.remove(this.objetos.size() - 1);
	  }

	  public boolean vazia() {
	    return this.objetos.size() == 0;
	  }
	}
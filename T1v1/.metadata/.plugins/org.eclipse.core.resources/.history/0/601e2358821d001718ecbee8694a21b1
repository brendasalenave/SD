import java.util.*;

public class Pilha {

	  private List<Integer> stack = new LinkedList<Integer>();

	  public void push(Integer n) {
	    this.stack.add(n);
	  }

	  public Integer pop() {
	    return this.stack.remove(this.stack.size() - 1);
	  }

	  public boolean vazia() {
	    return this.stack.size() == 0;
	  }
	}
import java.util.Stack;

public class Pilha{
    	
        private Stack stack;

		public Pilha(Stack p) {
            this.stack = p;
        }
    	
	    //Stack stack;
	    public synchronized void push(short valor){
	    	stack.push(valor);
	    }
	    
	    public synchronized short pop(){
	      	return (short) stack.pop();
	    }
    }